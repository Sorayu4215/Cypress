{
  "name": "testing-app",
  "version": "0.0.0",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "start_dev": "NODE_ENV=development node server.js",
    "server_start": "NODE_ENV=production nodemon server.js",
    "server_start_development": "NODE_ENV=development nodemon server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run testing-app:serve-ssr",
    "serve:ssr": "node dist/testing-app/server/main.js",
    "build:ssr": "ng build && ng run testing-app:server",
    "prerender": "ng run testing-app:prerender",
    "prerenderDev": "ng run testing-app:prerender:development",
    "cy:open": "npx cypress open --env envName=local",
    "cy:open:prod": "npx cypress open --config baseUrl='https://eshop-testing-app.herokuapp.com' --env envName=production",
    "cy:run:prod": "npx cypress run --config baseUrl='https://eshop-testing-app.herokuapp.com' --env envName=production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/platform-server": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@nguniversal/express-engine": "^15.2.1",
    "body-parse": "^0.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cypress": "^12.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.15.2",
    "express-fileupload": "^1.4.0",
    "font-awesome": "^4.7.0",
    "fortawesome": "^0.0.1-security",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "localstorage-polyfill": "^1.0.1",
    "mysql": "^2.18.1",
    "ngx-cookie-service": "^15.0.0",
    "ngx-meta": "^0.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular/cli": "~15.1.2",
    "@angular/compiler-cli": "^15.1.0",
    "@nguniversal/builders": "^15.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
